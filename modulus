import java.util.Scanner;
class Modulus 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the num1 value");
		int num1=sc.nextInt();
		System.out.println("enter the num2 value");
		int num2=sc.nextInt();
		int res=num1%num2;
		System.out.println(res);
	}
}
